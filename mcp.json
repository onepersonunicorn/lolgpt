{
  "name": "lol-mock-match",
  "version": "1.0.0",
  "description": "AI-powered League of Legends professional esports match predictor and summoner analysis tool. Predict outcomes for T1, Faker, Zeus, and other pro players with advanced statistical modeling.",
  "author": "LoL Mock Match Team",
  "homepage": "https://1tier.xyz",
  "repository": "https://github.com/onepersonunicorn/lolgpt",
  "license": "MIT",
  "categories": ["gaming", "esports", "simulation", "analytics", "professional-gaming", "competitive-analysis"],
  "keywords": ["mcp", "league-of-legends", "lol", "summoner", "mock-match", "simulation", "esports", "gaming", "riot-games", "summoners-rift", "prediction", "pvp", "comparison", "professional-gaming", "t1", "faker", "zeus", "lck", "lcs", "worlds", "msi", "pro-player", "esports-analytics", "competitive-lol", "team-analysis", "player-stats", "tournament-prediction"],
  "runtime": "python",
  "entrypoint": "main.py",
  "connection": {
    "type": "server",
    "command": "python",
    "args": ["main.py"]
  },
  "tools": [
    {
      "name": "professional_esports_match_predictor",
      "description": "Advanced match prediction for professional League of Legends players like T1 Faker, Zeus, and other LCK/LCS/Worlds competitors",
      "parameters": {
        "pro_player_1_id": {
          "type": "string",
          "description": "Professional player's Riot ID (e.g., 'Hide on bush' for Faker)",
          "required": true
        },
        "pro_player_1_tag": {
          "type": "string",
          "description": "Professional player's tag",
          "required": true
        },
        "pro_player_2_id": {
          "type": "string",
          "description": "Second professional player's Riot ID",
          "required": true
        },
        "pro_player_2_tag": {
          "type": "string",
          "description": "Second professional player's tag",
          "required": true
        },
        "tournament_context": {
          "type": "string",
          "description": "Tournament context (Worlds, MSI, LCK, LCS, etc.)",
          "default": "Professional Match"
        },
        "analysis_language": {
          "type": "string",
          "description": "Language for professional analysis",
          "default": "EN"
        }
      },
      "returns": "Professional esports match prediction with advanced analytics, performance metrics, and tournament-level insights"
    },
    {
      "name": "t1_faker_match_analysis",
      "description": "Specialized analysis tool for T1 team members including Faker, Zeus, and other T1 players",
      "parameters": {
        "t1_player_id": {
          "type": "string",
          "description": "T1 player's Riot ID",
          "required": true
        },
        "t1_player_tag": {
          "type": "string",
          "description": "T1 player's tag",
          "required": true
        },
        "opponent_id": {
          "type": "string",
          "description": "Opponent player's Riot ID",
          "required": true
        },
        "opponent_tag": {
          "type": "string",
          "description": "Opponent player's tag",
          "required": true
        },
        "match_type": {
          "type": "string",
          "description": "Type of match (Worlds Finals, LCK, etc.)",
          "default": "Professional"
        },
        "language": {
          "type": "string",
          "description": "Analysis language",
          "default": "EN"
        }
      },
      "returns": "Specialized T1 player analysis with historical performance, matchup predictions, and strategic insights"
    },
    {
      "name": "worlds_championship_predictor",
      "description": "Predict League of Legends World Championship matchups between professional players",
      "parameters": {
        "worlds_player_1_id": {
          "type": "string",
          "description": "First Worlds competitor's Riot ID",
          "required": true
        },
        "worlds_player_1_tag": {
          "type": "string",
          "description": "First Worlds competitor's tag",
          "required": true
        },
        "worlds_player_2_id": {
          "type": "string",
          "description": "Second Worlds competitor's Riot ID",
          "required": true
        },
        "worlds_player_2_tag": {
          "type": "string",
          "description": "Second Worlds competitor's tag",
          "required": true
        },
        "worlds_stage": {
          "type": "string",
          "description": "Worlds stage (Groups, Quarters, Semis, Finals)",
          "default": "Championship"
        },
        "prediction_language": {
          "type": "string",
          "description": "Language for Worlds prediction",
          "default": "EN"
        }
      },
      "returns": "World Championship level match prediction with tournament pressure analysis and championship insights"
    },
    {
      "name": "league_of_legends_summoner_vs_match",
      "description": "Simulate a League of Legends mock match between two summoners with detailed statistics and match progression",
      "parameters": {
        "uidA": {
          "type": "string",
          "description": "Riot ID of the first summoner",
          "required": true
        },
        "tagA": {
          "type": "string", 
          "description": "Tag of the first summoner",
          "required": true
        },
        "uidB": {
          "type": "string",
          "description": "Riot ID of the second summoner",
          "required": true
        },
        "tagB": {
          "type": "string",
          "description": "Tag of the second summoner", 
          "required": true
        },
        "lang": {
          "type": "string",
          "description": "Language for the simulation (EN, 한국어, 繁體中文, 日本語, ESPAÑOL, বাংলা, ਪੰਜਾਬੀ)",
          "default": "EN"
        }
      },
      "returns": "Detailed match simulation with summoner statistics from last 10 games and 10-phase match progression"
    },
    {
      "name": "lol_summoner_mock_battle",
      "description": "Create a League of Legends mock battle simulation between two summoners",
      "parameters": {
        "summoner1_id": {
          "type": "string",
          "description": "First summoner's Riot ID",
          "required": true
        },
        "summoner1_tag": {
          "type": "string",
          "description": "First summoner's tag",
          "required": true
        },
        "summoner2_id": {
          "type": "string", 
          "description": "Second summoner's Riot ID",
          "required": true
        },
        "summoner2_tag": {
          "type": "string",
          "description": "Second summoner's tag",
          "required": true
        },
        "language": {
          "type": "string",
          "description": "Simulation language",
          "default": "ENGLISH"
        }
      },
      "returns": "Complete battle simulation with stats and match progression"
    },
    {
      "name": "league_summoner_comparison",
      "description": "Compare two League of Legends summoners and simulate their matchup",
      "parameters": {
        "player1_riot_id": {
          "type": "string",
          "description": "First player's Riot ID",
          "required": true
        },
        "player1_tag": {
          "type": "string",
          "description": "First player's tag", 
          "required": true
        },
        "player2_riot_id": {
          "type": "string",
          "description": "Second player's Riot ID",
          "required": true
        },
        "player2_tag": {
          "type": "string",
          "description": "Second player's tag",
          "required": true
        },
        "output_language": {
          "type": "string",
          "description": "Language for output",
          "default": "EN"
        }
      },
      "returns": "Detailed comparison and mock match simulation"
    },
    {
      "name": "summoners_rift_simulation",
      "description": "Run a Summoner's Rift simulation between two League of Legends players",
      "parameters": {
        "riot_id_a": {
          "type": "string",
          "description": "Riot ID of first summoner",
          "required": true
        },
        "tag_a": {
          "type": "string",
          "description": "Tag of first summoner",
          "required": true
        },
        "riot_id_b": {
          "type": "string",
          "description": "Riot ID of second summoner", 
          "required": true
        },
        "tag_b": {
          "type": "string",
          "description": "Tag of second summoner",
          "required": true
        },
        "sim_language": {
          "type": "string",
          "description": "Language for simulation text",
          "default": "EN"
        }
      },
      "returns": "Full Summoner's Rift match simulation with phase-by-phase breakdown"
    },
    {
      "name": "lol_player_vs_player_match",
      "description": "Generate a League of Legends player vs player match simulation",
      "parameters": {
        "first_summoner_id": {
          "type": "string",
          "description": "First summoner's Riot ID",
          "required": true
        },
        "first_summoner_tag": {
          "type": "string",
          "description": "First summoner's tag",
          "required": true
        },
        "second_summoner_id": {
          "type": "string",
          "description": "Second summoner's Riot ID",
          "required": true
        },
        "second_summoner_tag": {
          "type": "string",
          "description": "Second summoner's tag",
          "required": true
        },
        "match_language": {
          "type": "string",
          "description": "Language for match narration",
          "default": "EN"
        }
      },
      "returns": "Complete PvP match simulation with statistics and battle progression"
    },
    {
      "name": "league_match_predictor",
      "description": "Predict the outcome of a League of Legends match between two summoners",
      "parameters": {
        "summoner_a_id": {
          "type": "string",
          "description": "First summoner's Riot ID",
          "required": true
        },
        "summoner_a_tag": {
          "type": "string",
          "description": "First summoner's tag",
          "required": true
        },
        "summoner_b_id": {
          "type": "string",
          "description": "Second summoner's Riot ID",
          "required": true
        },
        "summoner_b_tag": {
          "type": "string",
          "description": "Second summoner's tag",
          "required": true
        },
        "prediction_language": {
          "type": "string",
          "description": "Language for prediction output",
          "default": "ENGLISH"
        }
      },
      "returns": "Match prediction with detailed analysis and simulation"
    }
  ],
  "requirements": [
    "fastmcp>=0.1.0",
    "requests>=2.28.0"
  ],
  "python_version": ">=3.10",
  "configuration": {
    "debug": {
      "type": "boolean",
      "description": "Enable debug mode for detailed logging",
      "default": false
    },
    "lol_api_url": {
      "type": "string",
      "description": "Base URL for League of Legends API",
      "default": "https://1tier.xyz"
    }
  },
  "permissions": {
    "network": {
      "description": "Required to fetch summoner data and run match simulations from League of Legends API",
      "hosts": ["1tier.xyz"]
    }
  },
  "disclaimer": "League of Legends mock match simulations are for entertainment purposes only. Results are based on historical performance data and do not guarantee actual match outcomes. League of Legends is a trademark of Riot Games, Inc.",
  "tags": ["mcp", "league-of-legends", "lol", "summoner", "mock-match", "simulation", "esports", "gaming", "riot-games", "summoners-rift", "prediction", "pvp", "comparison", "professional-gaming", "t1", "faker", "zeus", "lck", "lcs", "worlds", "msi", "pro-player", "esports-analytics", "competitive-lol", "team-analysis", "player-stats", "tournament-prediction"]
